#
# ~/.xprofile
#

setxkbmap gb &			# Set keyboard to GB layout
xset r rate 300 50 &		# Speed up xrate
compton --blur-method kawase --blur-strength 18 --config ~/.config/compton/compton.conf	& # Start compositor
dunst &				# Start dunst for notifications
/usr/lib/kdeconnectd &		# KDE Connect Dameon
kdeconnect-indicator &		# KDE Connect applet
nm-applet &			# Network manager applet

